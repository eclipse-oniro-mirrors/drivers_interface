    GetCsRegStatusResponse([in] struct IHRilRadioResponseInfo responseInfo,
        [in] struct ICsRegStatusInfo csRegStatusInfo);
    GetPsRegStatusResponse([in] struct IHRilRadioResponseInfo responseInfo,
        [in] struct IPsRegStatusInfo psRegStatusInfo);
