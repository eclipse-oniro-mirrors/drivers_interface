    CallEmergencyNotice([in] int slotId, [in] struct IEmergencyInfoList emergencyInfoList);
    CallStateUpdated([in] int slotId, [in] int type);
    CallRingbackVoiceNotice([in] int slotId, [in] struct IRingbackVoice ringbackVoice);
    CallSrvccStatusNotice([in] int slotId, [in] struct ISrvccStatus srvccStatus);
    CallUssdNotice([in] int slotId, [in] struct IUssdNoticeInfo ussdNoticeInfo);
    CallSsNotice([in] int slotId, [in] struct ISsNoticeInfo ssNoticeInfo);

