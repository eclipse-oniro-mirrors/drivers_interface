    SetCdmaCBConfigResponse([in] struct IHRilRadioResponseInfo responseInfo);
    GetCdmaCBConfigResponse([in] struct IHRilRadioResponseInfo responseInfo,
        [in] struct ICdmaCBConfigInfo cdmaCBConfigInfo);
